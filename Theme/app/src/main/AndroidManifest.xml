<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.ds.theme.grey" >
	
	<uses-feature
		android:name="org.cyanogenmod.theme"
		android:required="true"/>
	<meta-data
		android:name="org.cyanogenmod.theme.name"
		android:value="N Style SystemUI"/>
    <meta-data
		android:name="org.cyanogenmod.theme.author"
		android:value="DSttr"/>
    <meta-data
		android:name="org.cyanogenmod.theme.email"
		android:value="dikisatris10@gmail.com"/>
	
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
